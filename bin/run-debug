#!/usr/bin/env zsh

cd "${0:h}"

export MAGIC_MODE=0
export DEBUG=1
# Public URL can be set to root for debug but needs
# to be set to _actual_ public base URL if reloaded.
export PUBLIC_URL="/"
export IMAGES_TO_TAG="./_data/images_to_tag"
export PIPELINE_OUTPUT="./_data/pipeline_output"

docker-compose up --build $@
#docker-compose logs -f $@
